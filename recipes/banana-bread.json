{
  "ingredients": {
    "butter": { "amount": 125, "condition": "softened" },
    "sugar": { "amount": 200, "type": "dark-muscavado" },
    "eggs": 2,
    "bananas": { "amount": 4, "type": "large", "condition": "ripe" },
    "vanilla": { "amount": 1, "unit": "tsp" },
    "espresso-powder": { "amount": 0.5, "unit": "tsp", "optional": true },
    "chocolate": { "amount": 100, "type": "80%" },
    "flour": { "amount": 250, "type": "plain" },
    "baking-powder": { "amount": 1, "unit": "tbsp" }
  },
  "substitutions": {
    "espresso-powder": { "coffee-powder": { "amount": 0.5, "unit": "tsp" } }
  },
  "time": {
    "preparation": 15,
    "baking": 50
  },
  "temperatures": {
    "oven": 180
  },
  "instructions": [
    "Heat oven",
    "In large bowl, cream butter and sugar together until light and fluffy",
    "Beat in the eggs one at a time",
    "In separate bowl, mash bananas lightly with a fork and stir in vanilla extract",
    "Chop chocolate finely",
    "Fold chocolate and banana batter in with butter mixture",
    "Add flour and baking powder gently",
    "Scrape into a non-stick baking tin"
  ]
}
